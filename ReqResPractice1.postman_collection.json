					"script": {
						"exec": [
							"pm.test(\"Validate basic values\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.total).to.eql(12);\r",
							"    pm.expect(jsonData.data[0].id).to.eql(7);\r",
							"});\r",
							"pm.test(\"Validate all the IDs\" ,function() {\r",
							"var jsondata = pm.response.json();\r",
							"var counter = jsondata.data.length;\r",
							"for(var i=0;i<counter;i++)\r",
							"{\r",
							"console.log(jsondata.data[i].id);\r",
							"}\r",
							"}\r",
							");\r",
							"\r",
							"postman.setNextRequest(null);"
						],
